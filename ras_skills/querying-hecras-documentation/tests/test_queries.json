    "content_extraction": {
      "min_text_length": 100,
      "requires_sections": true,
      "max_images_to_check": 10
    },
    "screenshot_quality": {
      "min_file_size_kb": 50,
      "max_file_size_mb": 10,
      "cache_valid_hours": 24
    }
